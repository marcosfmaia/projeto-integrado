Change={handleChange}\r\n                    />\r\n                </fieldset>\r\n\r\n                <button className=\"btn-cadastro\">Cadastrar</button>\r\n\r\n                {warning.show && <p className='register__failed'>{warning.message}</p>}\r\n\r\n                <div className=\"login-group__previus\">\r\n                    <i class=\"bi bi-arrow-left-short register__arrow\"></i>\r\n                    <Link to=\"/login\" className=\"register__login\">Voltar para login</Link>\r\n                </div>\r\n            </form>\r\n\r\n            <Footer />\r\n        </Container>\r\n    )\r\n}",[],"C:\\Users\\Remakker\\Documents\\dh\\projeto-integrador\\src\\components\\Footer\\index.jsx",[],[],"C:\\Users\\Remakker\\Documents\\dh\\projeto-integrador\\src\\components\\Main\\index.jsx",[],[],{"ruleId":"105","severity":1,"message":"106","line":9,"column":8,"nodeType":"107","messageId":"108","endLine":9,"endColumn":20},{"ruleId":"105","severity":1,"message":"109","line":8,"column":8,"nodeType":"107","messageId":"108","endLine":8,"endColumn":12},{"ruleId":"105","severity":1,"message":"110","line":5,"column":8,"nodeType":"107","messageId":"108","endLine":5,"endColumn":14},{"ruleId":"105","severity":1,"message":"109","line":8,"column":8,"nodeType":"107","messageId":"108","endLine":8,"endColumn":12},{"ruleId":"105","severity":1,"message":"111","line":12,"column":10,"nodeType":"107","messageId":"108","endLine":12,"endColumn":15},{"ruleId":"105","severity":1,"message":"112","line":17,"column":12,"nodeType":"107","messageId":"108","endLine":17,"endColumn":17},{"ruleId":"105","severity":1,"message":"113","line":3,"column":8,"nodeType":"107","messageId":"108","endLine":3,"endColumn":16},{"ruleId":"105","severity":1,"message":"109","line":7,"column":8,"nodeType":"107","messageId":"108","endLine":7,"endColumn":12},"no-unused-vars","'ModalDetails' is defined but never used.","Identifier","unusedVar","'Main' is defined but never used.","'Images' is defined but never used.",