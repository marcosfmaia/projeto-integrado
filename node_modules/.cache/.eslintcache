e === '' || form.password === '') {\r\n            setWarning({\r\n                show: true,\r\n                message: 'Preencha todos os campos'\r\n            });\r\n            setTimeout(() => {\r\n                setWarning({\r\n                    show: false,\r\n                    message: ''\r\n                });\r\n            }, 3000);\r\n            return;\r\n        }\r\n\r\n        let users = []\r\n\r\n        if(localStorage.getItem('users')) {\r\n            users = JSON.parse(localStorage.getItem('users'))\r\n        }\r\n        const user = users.find(u => u.email === form.email);\r\n        if(user) {\r\n            setWarning({\r\n                show: true,\r\n                message: 'E-mail já cadastrado'\r\n            });\r\n        } else {\r\n            users.push(form)\r\n            localStorage.setItem('users', JSON.stringify(users))\r\n            setForm({\r\n                name: '',\r\n                email: '',\r\n                telephone: '',\r\n                password: ''\r\n            })\r\n            setWarning({\r\n                show: true,\r\n                message: 'Usuario cadastrado com sucessso'\r\n            });\r\n            setTimeout(() => {\r\n                setWarning({\r\n                    show: false,\r\n                    message: ''\r\n                });\r\n            }, 10000);\r\n        }\r\n    }\r\n\r\n    return (\r\n        <Container>\r\n            <Header />\r\n        \r\n            <h2 className=\"title__registrer\">Crie sua Conta</h2>\r\n           \r\n            <form className=\"form-group\" onSubmit={handleSubmit}>\r\n                <fieldset className=\"form-group__cadastro\">\r\n                    <label htmlFor=\"user-nome\" className=\"label__text\">Nome</label>\r\n                    <i class=\"bi bi-person-fill login__icon\"></i>\r\n                    <input type=\"nome\" \r\n                    id=\"user-nome\"\r\n                    className=\"user-nome\" \r\